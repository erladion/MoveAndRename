<Window x:Class="MoveAndRename.SettingsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MoveAndRename"
        mc:Ignorable="d"
        Title="SettingsWindow" Height="400" Width="450">
    <Grid Name="ControlsGrid" SizeChanged="Grid_SizeChanged">
        <Grid Name="Content" HorizontalAlignment="Right" Margin="0,15,15,0" VerticalAlignment="Top" Width="240"/>
        <TreeView x:Name="treeView" HorizontalAlignment="Left" Height="100" Margin="15,15,0,0" VerticalAlignment="Top" Width="120" SelectedItemChanged="treeView_SelectedItemChanged"/>

    </Grid>
</Window>
